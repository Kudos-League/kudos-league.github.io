{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"w-32": {
		"style": {
			"width": 128
		}
	},
	"max-w-lg": {
		"style": {
			"maxWidth": 512
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"border-2": {
		"style": {
			"borderTopWidth": 2,
			"borderRightWidth": 2,
			"borderBottomWidth": 2,
			"borderLeftWidth": 2
		}
	},
	"border-black": {
		"style": {
			"--tw-border-opacity": 1,
			"borderTopColor": "rgb(0 0 0 / var(--tw-border-opacity))",
			"borderRightColor": "rgb(0 0 0 / var(--tw-border-opacity))",
			"borderBottomColor": "rgb(0 0 0 / var(--tw-border-opacity))",
			"borderLeftColor": "rgb(0 0 0 / var(--tw-border-opacity))"
		}
	}
}